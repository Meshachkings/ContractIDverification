                <div class="input-block">
                  <h4>Contract Information</h4>
                  <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="inputFname">Contract No.</label>
                            <input name="contract_name" type="text" class="form-control" id="inputFname">
                          </div>
                      <div class="form-group">
                        <label for="inputFname">Awarded To</label>
                        <input name="awarded_to" type="text" class="form-control" id="inputFname">
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="inputLname">Awarded By</label>
                        <input name="awarded_by" type="text" class="form-control" id="inputFname">
                        
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="state">State/Provience</label>
                    <input name="state_" type="text" class="form-control" id="inputFname">
                    
                  </div>
                  <div class="row">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="address1">Address</label>
                        <input name="address" type="text" class="form-control" id="inputFname">
                        
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="address2">Duration</label>
                        <input name="duration" type="text" class="form-control" id="inputFname">
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="city">Company</label>
                        <input name="company" type="text" class="form-control" id="inputFname">
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="zip">Contract Amount</label>
                        <input name="contract_amount" type="text" class="form-control" id="inputFname">
                        
                      </div>
                    </div>
                  </div>
                  <div class="file-input-wrap">
                    <div class="custom-file">
                      <input name="image" type="file" class="custom-file-input" id="nidf">
                      <label class="custom-file-label" for="nidf"><img src="images/cloud.png"></label>
                      <span class="text">Upload Document</span>
                    </div>
                    
                  </div>
                </div>
                <button class="btn">Post Document &nbsp; <img src="images/check.svg" alt=""></button>
