<%- include('./partials/header.ejs')  %>

    <div class="ugf-bg ufg-main-container">
      <div class="ugf-progress">
        <div class="progress">
          <div class="progress-bar" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
        </div>
      </div>
      <div class="container">
        <div class="row">
          <div class="col-lg-6 offset-lg-3">
            <div class="ugf-form">
              <form action="../<%= result.image  %>">
                <div class="input-block">
                  <h4>Contract Information</h4>
                  <div class="row">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="inputFname">Awarded To</label>
                        <p><%= result.awarded_to  %></p>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="inputLname">Awarded By</label>
                        <p><%= result.awarded_by %></p>
                        
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="state">State/Provience</label>
                    <p><%= result.state_ %></p>
                    
                  </div>
                  <div class="row">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="address1">Address</label>
                        <p><%= result.address %></p>
                        
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="address2">Duration</label>
                        <p><%= result.duration %></p>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="city">Company</label>
                        <p><%= result.company %></p>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="zip">Contract Amount</label>
                        <p><%= result.contract_amount %></p>
                        
                      </div>
                    </div>
                  </div>
                  <div class="form-group">
                    
                  </div>
                </div>
                <button class="btn">View Document &nbsp; </button>
              </form>
              <a href="/next" class="back-to-prev"><img src="images/arrow-left-grey.png" alt=""> Back to Previous</a>
            </div>
          </div>
          <div class="col-lg-3">
            <div class="uploaded-documents">
              <h4>Contract Documents</h4>
              <img width="50%" src="../<%= result.image %>" alt="">
              
            </div>
          </div>
        </div>
      </div>
    </div>

    <%- include('./partials/footer.ejs')  %>