  <body>

    <div class="ugf-nav">
      <div class="container">
        <div class="row">
          <div class="col">
            <div class="navigation">
              <div class="logo">
                <a href="/"><img src="images/logo-dark.png" class="img-fluid" alt=""><span>VERIFY</span></a>
              </div>
              <div class="nav-btns">
                <!-- <a href="/" class="back"><span class="text"></span></a> -->
                <a href="/" class="get">View Table</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="ugf-bg ufg-main-container">
      <div class="ugf-progress">
        <div class="progress">
          <div class="progress-bar" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
        </div>
      </div>
      <div class="container">
        
          <div class="">
            <div class="ugf-form">
                <div class="table-responsive">
                    <!-- Table with stripped rows -->
                    <table class="table table-striped ">
                      <thead>
                        <tr>
                          <th scope="col">#</th>
                          <th scope="col">Contract Number</th>
                          <th scope="col">Delete</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% result.forEach((item) => { %>
                          <tr>
                            <th scope="row"><%= id++  %></th>
                            <td><%= item.contract_name  %></td>
                            <td> 
                              <a href="/delete/<%= item._id  %>">
                                <button style="padding-left: 20px; padding-right: 20px; padding-bottom: 0px;" type="button" class="btn"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash-fill" viewBox="0 0 16 16">
                                  <path d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 1 0z"/>
                                </svg>
                                 Delete</button> 
                              </a>
                              </td>
                            </tr>
                        <% }) %>
                         
                          
                                   
                      </tbody>
                    </table>
                </div>
              <a href="/admin" class="back-to-prev"><img src="images/arrow-left-grey.png" alt=""> Back to Previous</a>
            </div>
          </div>
          <div class="col-lg-3">
            
          </div>
        
      </div>
    </div>

    <%- include('./partials/footer.ejs')  %>